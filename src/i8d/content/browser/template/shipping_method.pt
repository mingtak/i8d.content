<div xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="i8d.content"
    class="LogisticsType">


<tal:switch switch="view/request/form/value">
  <tal:case case="string:cvs">
    <metal:macro use-macro="context/@@i8d_macro/macros/cvs" />
  </tal:case>

  <tal:case case="string:home">
    <metal:macro use-macro="context/@@i8d_macro/macros/home" />
  </tal:case>
</tal:switch>

</div>
