<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="i8d.content">

    <configlet
        title="Cover setup"
        action_id="i8d.content.coverSetting"
        appId="i8d.content"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@coverSettingControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
